<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.facility.model">
    <class name="com.facility.model.FacilityUseImpl" table="facility_use">
        <id name="id" type="int">
            <column name="id" />
            <generator class="native" >
                <param name="sequence">id</param>
            </generator>
        </id>
        <property name="startDatetime" type="timestamp" column="start_datetime"/>
        <property name="endDatetime" type="timestamp" column="end_datetime"/>
        <property name="custId" type="java.math.BigDecimal" column="cust_id"/>
        <property name="status" type="string" column="status"/>
    </class>
</hibernate-mapping>

